package com.spring.henallux.sessionAttributeModel;

import java.util.ArrayList;

import com.spring.henallux.model.EquivalentCategory;

public class Categories {

	public String localeValue;
	public ArrayList<EquivalentCategory> categories;
	
	public Categories(){
		localeValue = null;
	}
	
	public void setNewLocale(String locale, ArrayList<EquivalentCategory> newCategories){
		this.localeValue = locale;
		setCategories(newCategories);
	}

	public ArrayList<EquivalentCategory> getCategories() {
		return categories;
	}

	public void setCategories(ArrayList<EquivalentCategory> categories) {
		this.categories = categories;
	}
	public String getLocaleValue() {
		return localeValue;
	}

	public void setLocaleValue(String localeValue) {
		this.localeValue = localeValue;
	}
	
}
